<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Method parameter intercept extension system for AspectCore Framework.</Description>
    <AssemblyTitle>AspectCore.Extensions.CrossParameters</AssemblyTitle>
    <VersionPrefix>1.1.0-rc1-13250</VersionPrefix>
    <Authors>Lemon</Authors>
    <TargetFrameworks>netstandard1.6;net45</TargetFrameworks>
    <AssemblyName>AspectCore.Extensions.CrossParameters</AssemblyName>
    <PackageId>AspectCore.Extensions.CrossParameters</PackageId>
    <PackageTags>Parameter;Aop;DynamicProxy;Interceptor</PackageTags>
    <PackageReleaseNotes>Method parameter intercept extension system for AspectCore Framework.</PackageReleaseNotes>
    <PackageIconUrl>https://avatars1.githubusercontent.com/u/19426425?v=3&amp;s=200</PackageIconUrl>
    <PackageProjectUrl>https://github.com/dotnetcore/AspectCore-Framework</PackageProjectUrl>
    <PackageLicenseUrl>https://mit-license.org/</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/dotnetcore/AspectCore-Framework</RepositoryUrl>
