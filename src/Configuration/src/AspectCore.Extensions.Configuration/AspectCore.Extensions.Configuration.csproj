<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\..\..\..\build\common.props" />
  <Import Project="..\..\..\..\build\net45.props" />
    
  <PropertyGroup>
    <Description>Configuration extension system for AspectCore Framework.</Description>
    <AssemblyTitle>AspectCore.Extensions.Configuration</AssemblyTitle>
    <Authors>Lemon</Authors>
    <TargetFrameworks>netstandard1.6;net45</TargetFrameworks>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <AssemblyName>AspectCore.Extensions.Configuration</AssemblyName>
    <PackageId>AspectCore.Extensions.Configuration</PackageId>
    <PackageTags>Configuration;Aop;DynamicProxy</PackageTags>
    <PackageReleaseNotes>Configuration extension system for AspectCore Framework.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AspectCore.Abstractions" Version="0.2.0-preview1" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="1.1.0" />
  </ItemGroup>

</Project>
