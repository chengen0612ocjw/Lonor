<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>ScopedContext extension system for AspectCore Framework.</Description>
    <AssemblyTitle>AspectCore.Extensions.ScopedContext</AssemblyTitle>
    <VersionPrefix>0.2.0</VersionPrefix>
    <Authors>Lemon</Authors>
    <TargetFrameworks>netstandard1.6;net46</TargetFrameworks>
    <AssemblyName>AspectCore.Extensions.ScopedContext</AssemblyName>
    <PackageId>AspectCore.Extensions.ScopedContext</PackageId>
    <PackageTags>DynamicProxy;Aop;Aspect;AspectCore;Intercepter</PackageTags>
    <PackageReleaseNotes>ScopedContext extension system for AspectCore Framework.</PackageReleaseNotes>
    <PackageIconUrl>https://avatars1.githubusercontent.com/u/19426425?v=3&amp;s=200</PackageIconUrl>
    <PackageProjectUrl>https://github.com/dotnetcore/AspectCore</PackageProjectUrl>
    <PackageLicenseUrl>https://mit-license.org/</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/dotnetcore/AspectCore</RepositoryUrl>
    <NetStandardImplicitPackageVersion Condition=" '$(TargetFramework)' == 'netstandard1.6' ">1.6.0</NetStandardImplicitPackageVersion>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>aspectcore.snk</AssemblyOriginatorKeyFile>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Version>0.2.0-preview1</Version>
    <DelaySign>False</DelaySign>
    <AssemblyVersion>0.2.0.0</AssemblyVersion>
    <FileVersion>0.2.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard1.6' ">
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'net46' ">
    <Reference Include="System" />
    <Reference Include="Microsoft.CSharp" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AspectCore.Core" Version="0.2.0-preview1" />
  </ItemGroup>

</Project>
